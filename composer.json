{
    "name": "mayconbordin/phprinter",
    "type": "library",
    "description": "PHP library for printers using PHP PECL, Internet Printing Protocol (IPP) and Line Printer Daemon Protocol (LPR)",
    "keywords": ["printer", "PECL", "IPP", "LPR"],
    "homepage": "https://github.com/mayconbordin/phprinter",
    "license": "MIT",
    "authors": [
        {
            "name": "Maycon Viana Bordin",
            "email": "mayconbordin@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "6.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.x"
    },
    "autoload": {
        "psr-4": {
            "Mayconbordin\\PhPrinter\\": "src/Mayconbordin/PhPrinter/"
        }
    },
    "minimum-stability": "dev"
}
